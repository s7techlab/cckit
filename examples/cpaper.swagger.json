        "paper_number": {
          "type": "string"
        },
        "owner": {
          "type": "string"
        },
        "issue_date": {
          "type": "string",
          "format": "date-time"
        },
        "maturity_date": {
          "type": "string",
          "format": "date-time"
        },
        "face_value": {
          "type": "integer",
          "format": "int32"
        },
        "state": {
          "$ref": "#/definitions/CommercialPaperState"
        },
        "external_id": {
          "type": "string",
          "title": "Additional unique field for entry"
        }
      },
      "title": "Commercial Paper state entry"
    },
    "cpaperCommercialPaperList": {
      "type": "object",
      "properties": {
        "items": {
          "type": "array",
