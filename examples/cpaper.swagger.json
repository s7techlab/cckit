        "STATE_TRADING",
        "STATE_REDEEMED"
      ],
      "default": "STATE_ISSUED"
    },
    "cpaperBuyCommercialPaper": {
      "type": "object",
      "properties": {
        "issuer": {
          "type": "string"
        },
        "paper_number": {
          "type": "string"
        },
        "current_owner": {
          "type": "string"
        },
        "new_owner": {
          "type": "string"
        },
        "price": {
          "type": "integer",
          "format": "int32"
        },
        "purchase_date": {
          "type": "string",
          "format": "date-time"
        }
      },
