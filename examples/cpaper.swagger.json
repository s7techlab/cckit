          }
        }
      },
      "title": "Container for returning multiple entities"
    },
    "cpaperIssueCommercialPaper": {
      "type": "object",
      "properties": {
        "issuer": {
          "type": "string"
        },
        "paper_number": {
          "type": "string"
        },
        "issue_date": {
          "type": "string",
